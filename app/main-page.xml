<Page xmlns="http://schemas.nativescript.org/tns.xsd" loaded="onLoaded" class="page">
    <Page.actionBar>
        <ActionBar title="HTTP GET Example" icon="" class="action-bar">
        </ActionBar>
    </Page.actionBar>

    <ScrollView>
        <StackLayout class="p-20">
            <Button text="HTTP GET" tap="onGetTap" class="btn btn-primary btn-active"/>
            <Label text="{{ httpResponse }}" textWrap="true" />
        </StackLayout>
    </ScrollView>
</Page>
